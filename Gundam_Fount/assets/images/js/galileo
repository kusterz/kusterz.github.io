$(function(){
    $(".G").addClass("hidden");
    $(".G:first").removeClass("hidden").addClass("current");

    $(".G").on("mouseout",function(){
    $(this).removeClass("current").addClass("hidden");
    $(this).next("img.G").removeClass("hidden").addClass("current");

       if($(".current").length == 0)
       {
           $(".G:first").removeClass("hidden").addClass("current");
       }


    })

})

